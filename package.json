{
  "name": "Golden Horizon -travel-template",
  "version": "1.0.0",
  "description": "Golden Horizon  Travel Template with SCSS compilation",
  "main": "index.html",
  "scripts": {
    "sass": "sass scss/style.scss css/style.css",
    "sass:watch": "sass --watch scss/style.scss:css/style.css",
    "sass:min": "sass scss/style.scss css/style.min.css --style=compressed",
    "sass:build": "npm run sass && npm run sass:min",
    "dev": "npm run sass:watch"
  },
  "keywords": [
    "travel",
    "template",
    "scss",
    "bootstrap"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.69.5"
  },
  "browserslist": [
    "last 5 versions"
  ]
}
